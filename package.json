{
  "name": "kenya-api",
  "version": "1.0.0",
  "description": "API documenting information about country Kenya",
  "repository": "https://github.com/dxphilo/kenya-api.git",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "biome lint --write",
    "format": "biome format --write",
    "check": "biome check --write",
    "test": "TEST_MODE=true tsx ./node_modules/mocha/bin/mocha.js \"tests/api_tests/**/*.test.ts\" --timeout 60000"
  },
  "keywords": [
    "kenya-api"
  ],
  "author": "John Philip <dxphilo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^5.2.3",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.14",
    "@types/express": "^5.0.6",
    "@types/node-cron": "^3.0.11",
    "@types/sinon": "^21.0.0",
    "@types/supertest": "^6.0.3",
    "chai": "^6.2.2",
    "sinon": "^21.0.1",
    "supertest": "^7.2.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/mocha": "^10.0.10",
    "axios": "^1.13.5",
    "body-parser": "^2.2.2",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "mocha": "^11.7.5",
    "node-cron": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
